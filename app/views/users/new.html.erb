<% provide(:title, 'Sign up') %>
<h1 id="right-margin">Sign Up</h1>
<div class= "row">
   <div class= "span8">
     <div class= "class-img img-rounded">
     </div> 	
   </div>
   <div class= "span4">
		<%= form_for(@user) do |f| %>
		<%= render 'shared/error_messages', object: f.object %>
		<%= render 'fields', f: f %>
		<%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>